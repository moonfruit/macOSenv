{
  "env": {
    "DISABLE_AUTOUPDATER": "1"
  },
  "permissions": {
    "allow": [
      "WebFetch(domain:docs.anthropic.com)"
    ],
    "deny": []
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/moon/Workspace.localized/env/bin/claude-hooks/notification.sh"
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "osascript -e \"display notification \\\"$(jq -r .message)\\\" with title \\\"$(jq -r .title)\\\" sound name \\\"Glass\\\"\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "/Users/moon/Workspace.localized/env/bin/claude-hooks/statusline.sh"
  },
  "alwaysThinkingEnabled": false,
  "verbose": false
}
