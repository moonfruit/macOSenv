<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>KeepAlive</key>
	<true/>
	<key>Label</key>
	<string>moonfruit.stunnel</string>
	<key>Program</key>
	<string>/usr/local/bin/stunnel</string>
	<key>RunAtLoad</key>
	<true/>
	<key>StandardErrorPath</key>
	<string>/tmp/moonfruit.stunnel.stderr</string>
	<key>StandardOutPath</key>
	<string>/tmp/moonfruit.stunnel.stdout</string>
</dict>
</plist>
